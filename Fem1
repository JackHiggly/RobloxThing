local function MultipleEx()
    game.Players.LocalPlayer:Kick("Do not execute twice. Script only loads when you Play in-game.")
    wait(15)
    game:Shutdown()
end

if game.PlaceId == 8343259840 then

    if _G.FemKess ~= "FemWare On Top" then
        Bypass()
    else
        game:GetService("StarterGui"):SetCore("SendNotification", {Title = "FemWare"; Text = "Script Executing... Script only loads when in-game."; Icon = "rbxassetid://9327507243"; Duration = 30 })
        wait(_G.Fem3k)
        if _G.Fem2k == _G.Fem3k then
            print("same")
        else
            print("pc exploded")
        end
        _G.FemKess = "FemWare on Top"
        if _G.Pass2Poo ~= "BAKAYAOO" and _G.FemKess ~= "FemWare on Top" then
            Bypass()
            while true do
                game:Shutdown()
                game:GetService("Players").LocalPlayer:Kick()
            end
        else
        end
    end
else
    game.Players.LocalPlayer:Kick("Execute only when you are in Criminality game, not lobby.")
    wait(15)
    game:Shutdown()
end
